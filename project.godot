[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/scale=4.0

[editor]

movie_writer/movie_file="./2024.4.20.avi"

[file_customization]

folder_colors={
"res://controllers/": "yellow",
"res://languages/": "gray"
}

